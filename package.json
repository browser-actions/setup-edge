  "description": "Set up your GitHub Actions workflow with a specific version of edge",
  "main": "dist/index.js",
  "packageManager": "pnpm@8.7.5",
  "engines": {
    "node": "20.6.1"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
